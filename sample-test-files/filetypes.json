{
    "fileMatches": [
        ["\\.html", "htmltype"],
        ["\\.css", "csstype"],
        ["\\.svg", "svgtype"],
        ["\\.webm", "webmtype"],
        [".*", "mytype"]
    ],
    "hierarchy": {
        "mytype": ["json", "js"]
    },
    "defaultHandlers": {
        "webmtype": {
            "binary-view": "http://brett-zamir.me/webappfind/webm.html?type=%type&method=%method&pathID=%pathID"
        },
        "csstype": {
            "edit": "http://brett-zamir.me/webappfind/css.html?editmode=true&type=%type&method=%method&pathID=%pathID"
        },
        "svgtype": {
            "edit": "http://brett-zamir.me/webappfind/svg.html?editmode=true&type=%type&method=%method&pathID=%pathID"
        },
        "htmltype": {
            "edit": "http://brett-zamir.me/webappfind/html.html?editmode=true&type=%type&method=%method&pathID=%pathID",
            "register": "http://example.com/register"
        },
        "mytype": {
            "view": "http://brett-zamir.me/webappfind/javascript.html?type=%type&method=%method&pathID=%pathID",
            "edit": "http://brett-zamir.me/webappfind/javascript.html?editmode=true&type=%type&method=%method&pathID=%pathID",
            "register": "http://example.com/register"
        },
        "json": {
            "view": "http://example.com/JSONview",
            "register": "http://example.com/JSONregister"
        }
    }
}
